	author = {Chen, Chi-Long and Chen, Chi-Chung and Yu, Wei-Hsiang and Chen, Szu-Hua and Chang, Yu-Chan and Hsu, Tai-I. and Hsiao, Michael and Yeh, Chao-Yuan and Chen, Cheng-Yu},
