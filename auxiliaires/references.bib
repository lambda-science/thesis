@article{smiyun_iii-tubulin_2017,
	title = {β{III}-tubulin enhances efficacy of cabazitaxel as compared with docetaxel},
	volume = {80},
	issn = {1432-0843},
	doi = {10.1007/s00280-017-3345-2},
	abstract = {Cabazitaxel is a novel taxane approved for treatment of metastatic hormone-refractory prostate cancer in patients pretreated with docetaxel. Cabazitaxel, docetaxel, and paclitaxel bind specifically to tubulin in microtubules, disrupting functions essential to tumor growth. High levels of β{III}-tubulin isotype expression are associated with tumor aggressivity and drug resistance. To understand cabazitaxel's increased efficacy, we examined binding of radio-labeled cabazitaxel and docetaxel to microtubules and the drugs' suppression of microtubule dynamic instability in vitro in microtubules assembled from purified bovine brain tubulin containing or devoid of β{III}-tubulin. We found that cabazitaxel suppresses microtubule dynamic instability significantly more potently in the presence of β{III}-tubulin than in its absence. In contrast, docetaxel showed no β{III}-tubulin-enhanced microtubule stabilization. We also asked if the selective potency of cabazitaxel on β{III}-tubulin-containing purified microtubules in vitro extends to cabazitaxel's effects in human tumor cells. Using {MCF}7 human breast adenocarcinoma cells, we found that cabazitaxel also suppressed microtubule shortening rates, shortening lengths, and dynamicity significantly more strongly in cells with normal levels of β{III}-tubulin than after 50\% reduction of β{III}-tubulin expression by {siRNA} knockdown. Cabazitaxel also more strongly induced mitotic arrest in {MCF}7 cells with normal β{III}-tubulin levels than after β{III}-tubulin reduction. In contrast, docetaxel had little or no β{III}-tubulin-dependent selective effect on microtubule dynamics or mitotic arrest. The selective potency of cabazitaxel on purified β{III}-tubulin-containing microtubules and in cells expressing β{III}-tubulin suggests that cabazitaxel may be unusual among microtubule-targeted drugs in its superior anti-tumor efficacy in tumors overexpressing β{III}-tubulin.},
	pages = {151--164},
	number = {1},
	journaltitle = {Cancer Chemotherapy and Pharmacology},
	author = {Smiyun, Gregoriy and Azarenko, Olga and Miller, Herbert and Rifkind, Alexander and {LaPointe}, Nichole E. and Wilson, Leslie and Jordan, Mary Ann},
	date = {2017},
	pmid = {28567478},
	keywords = {Adenocarcinoma, Animals, Antineoplastic Agents, Brain, Breast Neoplasms, Cabazitaxel, Cattle, Docetaxel, Drug Resistance, Female, Gene Expression Regulation, Gene Knockdown Techniques, Humans, {MCF}-7 Cells, Microtubule, Microtubules, Neoplasm, Neoplastic, {RNA}, Small Interfering, Taxane, Taxoids, Tubulin, Tubulin Modulators, β{III}-tubulin},
}

