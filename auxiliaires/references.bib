@inproceedings{vega_translational_2022,
	title = {Translational challenges of biomedical machine learning solutions in clinical and laboratory settings},
	doi = {10.5281/zenodo.5776141},
	abstract = {The ever increasing use of {AI} methods in biomedical sciences calls for closer inter-disciplinary collaborations that transfer the domain knowledge from life scientists to {IT} researchers and vice-versa.
