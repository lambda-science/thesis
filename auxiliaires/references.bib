	keywords = {Aged, Antineoplastic Agents, Biomarkers, Cell Line, Cell Survival, Docetaxel, Drug Resistance, Humans, Immunoblotting, Immunohistochemistry, Male, Middle Aged, Multivariate Analysis, Neoplasm, Neoplasm Invasiveness, Orchiectomy, Prognosis, Prostate, Prostatectomy, Prostatic Neoplasms, {RNA} Interference, Survival Analysis, Taxoids, Tubulin, Tumor},
