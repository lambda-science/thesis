	number = {3},
	journaltitle = {European Journal of Neurology},
	shortjournal = {Eur J Neurol},
	author = {Sanga, Shamita and Ghosh, Arnab and Kumar, Krishna and Polavarapu, Kiran and Preethish-Kumar, Veeramani and Vengalil, Seena and Nashi, Saraswati and Bardhan, Mainak and Arunachal, Gautham and Raju, Sanita and Gayathri, Narayanappa and Biswas, Nidhan K. and Chakrabarti, Saikat and Nalini, Atchayaram and Roy, Sudipto and Acharya, Moulinath},
