@article{kuttapitiya_microarray_2017,
	title = {Microarray analysis of bone marrow lesions in osteoarthritis demonstrates upregulation of genes implicated in osteochondral turnover, neurogenesis and inflammation},
	volume = {76},
	issn = {1468-2060},
	doi = {10.1136/annrheumdis-2017-211396},
	abstract = {{OBJECTIVE}: Bone marrow lesions ({BMLs}) are well described in osteoarthritis ({OA}) using {MRI} and are associated with pain, but little is known about their pathological characteristics and gene expression. We evaluated {BMLs} using novel tissue analysis tools to gain a deeper understanding of their cellular and molecular expression. {METHODS}: We recruited 98 participants, 72 with advanced {OA} requiring total knee replacement ({TKR}), 12 with mild {OA} and 14 non-{OA} controls. Participants were assessed for pain (using Western Ontario and {McMaster} Universities Osteoarthritis Index ({WOMAC})) and with a knee {MRI} (using {MOAKS}). Tissue was then harvested at {TKR} for {BML} analysis using histology and tissue microarray. {RESULTS}: The mean ({SD}) {WOMAC} pain scores were significantly increased in advanced {OA} 59.4 (21.3) and mild {OA} 30.9 (20.3) compared with controls 0.5 (1.28) (p{\textbackslash}textless0.0001). {MOAKS} showed all {TKR} tissue analysed had {BMLs}, and within these lesions, bone marrow volume was starkly reduced being replaced by dense fibrous connective tissue, new blood vessels, hyaline cartilage and fibrocartilage. Microarray comparing {OA} {BML} and normal bone found a significant difference in expression of 218 genes (p{\textbackslash}textless0.05). The most upregulated genes included stathmin 2, thrombospondin 4, matrix metalloproteinase 13 and Wnt/Notch/catenin/chemokine signalling molecules that are known to constitute neuronal, osteogenic and chondrogenic pathways. {CONCLUSION}: Our study is the first to employ detailed histological analysis and microarray techniques to investigate knee {OA} {BMLs}. {BMLs} demonstrated areas of high metabolic activity expressing pain sensitisation, neuronal, extracellular matrix and proinflammatory signalling genes that may explain their strong association with pain.},
	pages = {1764--1773},
	number = {10},
	journaltitle = {Annals of the Rheumatic Diseases},
	author = {Kuttapitiya, Anasuya and Assi, Lena and Laing, Ken and Hing, Caroline and Mitchell, Philip and Whitley, Guy and Harrison, Abiola and Howe, Franklyn A. and Ejindu, Vivian and Heron, Christine and Sofat, Nidhi},
	date = {2017-10},
	pmid = {28705915},
	pmcid = {PMC5629942},
	keywords = {80 and over, Adult, Aged, Arthroplasty, Bone Marrow, Bone Remodeling, Chemokines, Chondrocytes, Chondrogenesis, Female, Gene Expression Profiling, Humans, Inflammation, Knee, Knee Osteoarthritis, Magnetic Resonance Imaging, Male, Middle Aged, Neurogenesis, Osteoarthritis, Osteogenesis, Pain Measurement, Replacement, Severity of Illness Index, Tissue Array Analysis, Up-Regulation, Young Adult},
